! Base16 Default
! Scheme: Chris Kempson (http://chriskempson.com)

#define base00 #181818
#define base01 #282828
#define base02 #383838
#define base03 #585858
#define base04 #b8b8b8
#define base05 #d8d8d8
#define base06 #e8e8e8
#define base07 #f8f8f8
#define base08 #ab4642
#define base09 #dc9656
#define base0A #f7ca88
#define base0B #a1b56c
#define base0C #86c1b9
#define base0D #7cafc2
#define base0E #ba8baf
#define base0F #a16946

*.foreground:   base05
*.background:   base00
*.cursorColor:  base05

*.color0:       base00
*.color1:       base08
*.color2:       base0B
*.color3:       base0A
*.color4:       base0D
*.color5:       base0E
*.color6:       base0C
*.color7:       base05

*.color8:       base03
*.color9:       base08
*.color10:      base0B
*.color11:      base0A
*.color12:      base0D
*.color13:      base0E
*.color14:      base0C
*.color15:      base07

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
*.color16:      base09
*.color17:      base0F
*.color18:      base01
*.color19:      base02
*.color20:      base04
*.color21:      base06

*cursorColor:  #00ff00

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

! General

!*font:         -lucy-Tewi-medium-R-Normal--11-90-75-75-P-58-ISO10646-1
*font:        -misc-fixed-medium-r-semicondensed-*-12-120-75-75-c-60-iso10646-1
Xcursor*theme: neutralplus-inv

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

XTerm*dynamicColors:      true
XTerm*locale:             true
XTerm*metaSendsEscape:    true
XTerm*font:               -misc-fixed-medium-r-semicondensed-*-12-120-75-75-c-60-iso10646-1
XTerm*faceSize:           8
XTerm*faceNameDoublesize: WenQuanYi Bitmap Song
XTerm*saveLines:          16384
! make Backspace generate ^? instead ^H, which seems to work better for most things
XTerm*backarrowKey:       false

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

!#define fonts 
!#define fonts -lucy-tewi-medium-r-normal--11-90-75-75-p-58-iso10646-1
!#define fonts xft:MS Gothic:pixelsize=12
!#define fonts -*-proggyclean-*-*-*-*-13-*-*-*-*-*-*-*
#define fonts -misc-fixed-medium-r-semicondensed-*-12-120-75-75-c-60-iso10646-1

URxvt*font:           fonts
URxvt*boldFont:       fonts
URxvt*italicFont:     fonts
URxvt*bolditalicFont: fonts

URxvt*cursorBlink     : false
! true transparency for compositor
URxvt*depth           : 32
! this usually means use the desktop background as the background instead of a
! configured color for pseudo-transparency
URxvt*inheritPixmap   : false
URxvt*intensityStyles : true
URxvt*internalBorder  : 1
URxvt*loginShell      : true
URxvt*meta8           : false
URxvt*pointerBlank    : true
URxvt*saveLines       : 8192
URxvt*scrollBar       : false
URxvt*skipScroll      : true
URxvt*transparent     : false
URxvt*urgentOnBell    : true
URxvt*visualBell      : false
URxvt*iso14755        : false
URxvt*reverseVideo    : false
URxvt*cursorUnderline : true
URxvt*fading          : 0
